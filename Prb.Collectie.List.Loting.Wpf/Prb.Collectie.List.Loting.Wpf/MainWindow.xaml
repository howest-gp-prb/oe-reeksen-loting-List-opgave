        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prb.Collectie.List.Loting.Wpf"
        mc:Ignorable="d"
        Title="Loting" Height="380" Width="720" Loaded="wndLoting_Loaded">
    <Grid>
        <ListBox x:Name="lstBeschikbaar" HorizontalAlignment="Left" Height="236" Margin="26,50,0,0" VerticalAlignment="Top" Width="150" IsEnabled="False"/>
