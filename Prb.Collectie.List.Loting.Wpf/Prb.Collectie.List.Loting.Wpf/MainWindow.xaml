        <ListBox x:Name="lstMatchen" HorizontalAlignment="Left" Height="236" Margin="382,50,0,0" VerticalAlignment="Top" Width="318" IsEnabled="False"/>
        <Button x:Name="btnLoot" Content="Loot" HorizontalAlignment="Left" Margin="26,302,0,0" VerticalAlignment="Top" Width="75" />
        <Label Content="In de trommel" HorizontalAlignment="Left" Margin="26,10,0,0" VerticalAlignment="Top" Width="150"/>
        <Label Content="Uit de trommel" HorizontalAlignment="Left" Margin="200,10,0,0" VerticalAlignment="Top" Width="150"/>
        <Label Content="Wedstrijden" HorizontalAlignment="Left" Margin="382,10,0,0" VerticalAlignment="Top" Width="318"/>
    </Grid>
</Window>
